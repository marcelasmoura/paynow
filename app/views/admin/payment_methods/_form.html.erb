<%= form_for [:admin, @payment_method] do |f| %>
  <%= f.label :name %>
    <%= f.text_field :name %>
  <%= f.label :charge_fee %>
    <%= f.text_field :charge_fee %>
  <%= f.label :minimum_fee %>
    <%= f.text_field :minimum_fee %>
  <%= f.label :payment_icon %>
    <%= f.file_field :payment_icon %>
  <%= f.label :available %>
    <%= f.check_box :available %>
  <%= f.submit 'Salvar' %>
<% end %>